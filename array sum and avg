
#include <stdio.h>
int main ()
{
    int a[10],i,n,s=0,avg=0;
    printf("enter the element");
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
        printf("enter a[%d]",i);
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        s+=a[i];
        avg=s/n;
        printf(" sum : %d\n avg : %d",s,avg);
    }
    return 0;
}
