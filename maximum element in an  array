
#include <stdio.h>
int main ()
{
    int a[10],i,n;
    printf("enter the limit");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter a[%d]",i);
        scanf("%d",&a[i]);
    }
    int max=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
        printf("max elemnt : %d",max);
    }

    
    return 0;
    
}
